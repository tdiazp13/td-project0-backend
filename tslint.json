{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      false
    ],
    "no-console": [
      true,
      "log"
    ],
    "whitespace": [
      true,
      "check-module"
    ],
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "no-require-imports": [
      false
    ],
    "no-var-requires": [
      false
    ]
  },
  "rulesDirectory": []
}